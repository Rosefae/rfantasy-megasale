{
  "name": "rfantasy-megasale-page",
  "version": "1.0.0",
  "description": "A page to more easily browse the books on sale for the annual r/fantasy megasale",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "once": "npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "oncescss": "gulp scss",
    "scss": "gulp",
    "prodscss": "gulp prodscss",
    "start": "concurrently -k -c green.dim,cyan.dim -s first \"npm:serve\" \"npm:scss\"",
    "minify-js": "terser src/_scripts/script.js -c -m --output dist/_scripts/script.js",
    "build": "npm run once && npm run prodscss && npm run minify-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rosefae/rfantasy-megasale.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rosefae/rfantasy-megasale/issues"
  },
  "homepage": "https://github.com/Rosefae/rfantasy-megasale#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.0",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "postcss": "^8.4.24",
    "sass": "^1.63.4"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "terser": "^5.44.1"
  }
}
